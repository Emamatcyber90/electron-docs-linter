    "entities": "^1.1.1",
    "keyed-array": "^2.1.0",
    "lodash.pick": "^4.2.1",
    "lodash.sum": "^4.0.2",
    "marky-markdown-lite": "^1.2.0",
    "minimist": "^1.2.0",
